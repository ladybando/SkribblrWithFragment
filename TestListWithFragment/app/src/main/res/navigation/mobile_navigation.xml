<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_list">

    <fragment
        android:id="@+id/nav_list"
        android:name="com.example.android.testlistwithfragment.fragments.ListItemFragment"
        android:label="@string/menu_transform"
        tools:layout="@layout/fragment_list_item" />

    <fragment
        android:id="@+id/nav_new"
        android:name="com.example.android.testlistwithfragment.newItem.NewItemFragment"
        android:label="@string/menu_reflow"
        tools:layout="@layout/fragment_new_item" />

    <fragment
        android:id="@+id/nav_recycler"
        android:name="com.example.android.testlistwithfragment.recycler.RecyclerFragment"
        android:label="view"
        tools:layout="@layout/fragment_recycler"/>
</navigation>